@page "/contacts"

@inject WebpageData data


<h3>Kontakty</h3>

<div class="container">
    @foreach(var c in data.Contacts)
    {
        <div class="card">
            <div class="card-header">@($"Jméno:   {c.PersonName}"</div>
            <div class="card-body">@($"E-mail:   {c.Email}"</div>
            <div class="card-body">@($"Telefon:   {c.PhoneNumber}"</div>
        </div>
    }
</div>

